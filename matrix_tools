# add
def add(X, Y):
    assert len(X) == len(Y)
    assert len(X[0]) == len(Y[0])


    n = len(X)
    m = len(X[0])

    Z = [[0 for c in range(m)] for r in range(n)]

    for r in range(n):
        for c in range(m):
            Z[r][c] = X[r][c] + Y[r][c]

    return Z
            
# multiply

# display
def display(X):
    for row in X:
        print(row)
